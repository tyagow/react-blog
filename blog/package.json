{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "4.1.0",
    "chai": "^4.1.2",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "seamless-immutable": "^7.1.3",
    "sinon": "^4.5.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test":
      "react-scripts test --env=jsdom --setupFiles='./src/tests/setupTest.js'",
    "test:coverage":
      "react-scripts test --coverage --env=jsdom --setupFiles='./src/tests/setupTest.js'",
    "posttest":
      "react-scripts test --coverage --env=jsdom --setupFiles='./src/tests/setupTest.js",
    "eject": "react-scripts eject",
    "precommit": "pretty-quick --staged"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "fetch-mock": "^6.3.0",
    "husky": "^0.14.3",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "lint-staged": "^7.0.0",
    "prettier": "1.8.0",
    "pretty-quick": "^1.4.1",
    "react-scripts": "^1.1.1",
    "react-timestamp": "^4.4.0",
    "redux-mock-store": "^1.5.1",
    "redux-observable": "^0.18.0",
    "redux-tdd": "^1.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,sx}",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/tests/setupTest.js",
      "!index.js",
      "!src/store/store.js"
    ],
    "snapshotSerializers": ["enzyme-to-json/serializer"]
  }
